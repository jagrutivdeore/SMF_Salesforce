     @HttpPost
    global static void doPost(String name, String Cluster, String District, String Email, String Password, String Phone, String Roll, String SchoolName,String SchoolCode, String Taluka, String Village) {
        
       
       RestResponse res = RestContext.response;
       Map<String,String> mapResponse = new Map<String,String>();
      // mapResponse.put('Success', 'True');
       //mapResponse.put('RandomNumber', randomNumber);
       //res.responseBody = Blob.valueOf(JSON.serialize(mapResponse));


       
       list<User__c> userlist = [select id,Phone__c from User__c where Phone__c =: Phone ]; 
       system.debug('userlist'+userlist.size());
       integer userlistsize = userlist.size(); 
       if( userlistsize == 0 ){
            User__c user = new User__c();
            user.Name            = name;
            user.Cluster__c      = Cluster;
            user.District__c     = District;
            user.Email__c        = Email;
            user.Password__c     = Password;
            user.Phone__c        = Phone;
            user.Roll__c         = Roll;
            user.School_Code__c  = SchoolCode;
            user.School_Name__c  = SchoolName;
            user.Taluka__c       = Taluka;
            user.Village__c      = Village;
            
            insert user;
            
            mapResponse.put('Success','True');
            mapResponse.put('Id',user.id);
            res.responseBody = Blob.valueOf(JSON.serialize(mapResponse));

            // return res;
       }
       
       else{
               mapResponse.put('Success','False');
               mapResponse.put('Message', 'Phone number already exist');
               res.responseBody = Blob.valueOf(JSON.serialize(mapResponse));
                
               // return res;
               //return 'Failed to submit Phone number already exist';
       }
    }
    
