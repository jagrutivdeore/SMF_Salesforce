@RestResource(urlMapping='/getAttachmentBody/*')
global class WS_getAttachmentBody {
  
    @HttpGet
    global static String doGet() {
        RestRequest req = RestContext.request;
        RestResponse res = RestContext.response;
        Attachment output =  new Attachment();
        String attId = req.requestURI.substring(req.requestURI.lastIndexOf('/')+1);
        output = [SELECT Id,body FROM Attachment where Id =:attId];
        String bodyContent = EncodingUtil.base64Encode(output.body);
       return bodyContent;
     
   }
