@RestResource(urlMapping='/getApprovalData/*')
global with sharing class WS_getApprovalData {

    @HttpGet
    global static List<approvalData> doGet() {
        RestRequest req = RestContext.request;
        RestResponse res = RestContext.response;
        String userId = RestContext.request.params.get('userId');
        MV_User__c user = new MV_User__c();
        user = [SELECT Id,MV_Role__c,MV_Role__r.Name,User_District__c,User_Taluka__c,User_State__c From MV_User__c where id = :userId];
        List<MV_Role__c> roleList = new List<MV_Role__c>();
        roleList = [SELECT Id,Approval_person_Role__c,Name FROM MV_Role__c where Approval_person_Role__c includes (:user.MV_Role__r.Name)];
        
        set<String> strset = new set<String>();
        for(MV_Role__c role : roleList){
            strset.add(role.Name);
        }
        List<MV_User__c> userList = new List<MV_User__c>();
        
        System.debug('Dist Is ----------' + user.User_District__c );
        System.debug('Taluka is---------'+user.User_Taluka__c);
        
        if((user.User_District__c == 'select' || user.User_District__c == '' || user.User_District__c == null ) && (user.User_Taluka__c == 'select' || user.User_Taluka__c == null || user.User_Taluka__c == '')){
            //userList = [SELECT Id,Name,Last_Name__c, Is_Approved__c FROM MV_User__c where Role_Name__c IN :strset AND Is_Approved__c = false AND (User_State__c = :user.User_State__c)];
            userList = [SELECT Id,Name,Last_Name__c, Is_Approved__c,Approver_Comment__c FROM MV_User__c where (Role_Name__c IN :strset AND (User_State__c = :user.User_State__c)) or (Super_Visor_Name__c =: userId) ORDER BY Name  Asc];
            
        }else{
            //userList = [SELECT Id,Name,Last_Name__c, Is_Approved__c FROM MV_User__c where Role_Name__c IN :strset AND Is_Approved__c = false AND (User_District__c = :user.User_District__c OR User_Taluka__c = :user.User_Taluka__c )];
            userList = [SELECT Id,Name,Last_Name__c, Is_Approved__c,Approver_Comment__c FROM MV_User__c where (Role_Name__c IN :strset AND (User_District__c = :user.User_District__c OR User_Taluka__c = :user.User_Taluka__c )) or (Super_Visor_Name__c =: userId) ORDER BY Name  Asc];
        }
        
        
        List<approvalData> approvalDataList = new List<approvalData>();
        for(MV_User__c user1 : userList){
            approvalData ad = new approvalData();
            
            if(user1.Name != null && user1.Name != ''){
                ad.username = user1.Name + ' ';
            }
            if(user1.Last_Name__c != null && user1.Last_Name__c != ''){
                ad.username = ad.username + user1.Last_Name__c;
            }
            ad.status = string.valueOf(user1.Is_Approved__c);
            if(user1.Approver_Comment__c != null && user1.Approver_Comment__c != '')
           {
                  ad.status = 'Rejected';
                }
            
            ad.Id = user1.Id;
            //.status = user1.Is_Approved__c;
            approvalDataList.add(ad);
        }
        return approvalDataList;
    }
      global class approvalData{
        String username;
        String Id;
        String status;
      }
      
