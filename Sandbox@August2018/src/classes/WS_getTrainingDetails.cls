@RestResource(urlMapping='/getTrainingDetails/*')
global class WS_getTrainingDetails{
  
    @HttpGet
    global static List<Fields> doGet() {

    RestRequest req = RestContext.request;
    RestResponse res = RestContext.response;
    String district = RestContext.request.params.get('district');
     String taluka = RestContext.request.params.get('taluka');
     String cluster = RestContext.request.params.get('cluster');
     List<Fields> outputList = new List<Fields>();
     system.debug('District : '+district  );
     system.debug('Taluka : '+taluka );
     system.debug('Cluster : '+cluster );
    List<MV_Training_Calender__c > groupedResults = [SELECT Code__c,Trainer1__c,Trainer2__c,Trainer3__c,Batch__c, Id FROM MV_Training_Calender__c where District__c = :district AND Taluka__c = :taluka AND Cluster__c = :cluster];
     system.debug('Size : '+groupedResults.size() );
    for (MV_Training_Calender__c ar : groupedResults)  {
       Fields fl = new Fields();
        fl.id= ar.Id;
        fl.BatchNo = ar.Batch__c;
    fl.Code = ar.Code__c;
    fl.Trainer1 = ar.Trainer1__c;
    fl.Trainer2 = ar.Trainer2__c;
    fl.Trainer3 = ar.Trainer3__c;
        outputList.add(fl);
    }
    return outputList;
}


