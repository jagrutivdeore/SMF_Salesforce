    tskList = [SELECT Id,Validaytion_on_text__c,Name,Is_Completed__c,Is_Response_Mnadetory__c,Picklist_Value__c,MV_Process__c,Task_Response__c,Task_type__c,Task_Text__c,CreatedDate,Position__c,Location_Level__c from MV_Task__c where MV_Process__c = :Id AND IsActived__c = true ORDER BY Position__c];
    
