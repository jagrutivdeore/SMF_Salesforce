@RestResource(urlMapping='/InsertComments/*')
global class WS_InsertComments {
    
    @HttpPost
    global static void InsertComments(){
        RestRequest req = RestContext.request;
        RestResponse res = RestContext.response;
        res.addHeader('Content-Type', 'application/json'); 
        
        Map<String,List<Object>> mapResponse = new Map<String,List<Object>>();
        CompostPits mCompostPits = new CompostPits();
        
        mCompostPits = (CompostPits)JSON.deserialize(req.requestBody.toString(),CompostPits.class);

        List<MV_Content_Comment__c> compostPitsList = mCompostPits.listOfComments;
        
        try{
              Database.UpsertResult[] srList = Database.upsert(compostPitsList,true);
             // System.debug('***************************************'+srList +'************************************'+compostPitsList);
             
             List<String> commentId = new List<String>();
             for(MV_Content_Comment__c c : compostPitsList)
             {
               commentId.add(c.Id);
             } 
             
             for(MV_Content_Comment__c commObj : [Select Id,Comment__c,User_Name__c,MV_Content__r.MV_User__c,MV_Content__r.MV_User__r.Name,MV_Content__c,MV_Content__r.Title__c,MV_User__c,MV_Content__r.MV_Community__r.Name,CreatedDate from MV_Content_Comment__c where Id IN : commentId])
             {
               System.debug('------------------------Notification');
               List<String> userApprovalList = new List<String>();
               userApprovalList.add(commObj.MV_Content__r.MV_User__c);
               String D1 =(commObj.CreatedDate).format();
               if(commObj.MV_Content__r.MV_User__c != commObj.MV_User__c)
               sendMultipleNotificationMsg.submitdata(userApprovalList,commObj.User_Name__c+' Comments on your '+commObj.MV_Content__r.MV_Community__r.Name+'\'s post',String.valueOf(D1) +' '+commObj.Comment__c);
             }
             
               mapResponse.put('Records', compostPitsList);
                res.responseBody = Blob.valueOf(JSON.serialize(mapResponse));
                

        }catch(Exception e){

            res.responseBody = Blob.valueOf(JSON.serialize(e.getMessage()));
        }
    }
    
